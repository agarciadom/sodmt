<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.epsilon.evl.emf.validation">
      <constraintsBinding
            constraints="validation/common.evl"
            namespaceURI="www.uca.es/modeling/serviceProcess">
      </constraintsBinding>
      <constraintsBinding
            constraints="validation/serviceprocess.evl"
            namespaceURI="www.uca.es/modeling/serviceProcess">
      </constraintsBinding>
      <constraintsBinding
            constraints="validation/common.evl"
            namespaceURI="www.uca.es/modeling/serviceComposition">
      </constraintsBinding>
      <constraintsBinding
            constraints="validation/servicecomposition.evl"
            namespaceURI="www.uca.es/modeling/serviceComposition">
      </constraintsBinding>
   </extension>
   <extension
         point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            class="org.eclipse.epsilon.evl.emf.validation.EvlMarkerResolutionGenerator"
            markerType="org.eclipse.emf.ecore.diagnostic">
      </markerResolutionGenerator>
      <markerResolutionGenerator
            class="org.eclipse.epsilon.evl.emf.validation.EvlMarkerResolutionGenerator"
            markerType="ServiceProcess.diagram.diagnostic">
      </markerResolutionGenerator>
      <markerResolutionGenerator
            class="org.eclipse.epsilon.evl.emf.validation.EvlMarkerResolutionGenerator"
            markerType="ServiceComposition.diagram.diagnostic">
      </markerResolutionGenerator>
   </extension>
   <extension
         point="org.eclipse.epsilon.ewl.emf.wizards">
      <wizards
            file="wizards/processWizards.ewl"
            namespaceURI="www.uca.es/modeling/serviceProcess">
      </wizards>
      <wizards
            file="wizards/compositionWizards.ewl"
            namespaceURI="www.uca.es/modeling/serviceComposition">
      </wizards>
   </extension>
   

</plugin>
