<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="es.uca.modeling.sodmt.wizards"
            name="%_UI_Wizard_category"/>
      <wizard
            id="serviceComposition.presentation.ServiceCompositionModelWizardID"
            name="%_UI_ServiceCompositionModelWizard_label"
            class="serviceComposition.presentation.ServiceCompositionModelWizard"
            category="es.uca.modeling.sodmt.wizards"
            icon="icons/full/obj16/ServiceCompositionModelFile.gif">
         <description>%_UI_ServiceCompositionModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="serviceComposition.presentation.ServiceCompositionEditorID"
            name="%_UI_ServiceCompositionEditor_label"
            icon="icons/full/obj16/ServiceCompositionModelFile.gif"
            extensions="servicecomposition"
            class="serviceComposition.presentation.ServiceCompositionEditor"
            contributorClass="serviceComposition.presentation.ServiceCompositionActionBarContributor">
      </editor>
   </extension>

</plugin>
