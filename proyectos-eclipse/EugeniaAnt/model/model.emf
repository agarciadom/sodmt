@namespace(uri="sampleModel", prefix="sm")
package model;

@gmf.diagram(foo="bar")
class Diagram {
   val Node[*] node;
   val Edge[*] edge;
}

@gmf.node(label="name")
class Node {
  attr String name;
  ref Edge[*]#source outgoing;
  ref Edge[*]#target incoming;   
}

@gmf.link(label="name")
class Edge {
   ref Node#outgoing source;
   ref Node#incoming target;
}