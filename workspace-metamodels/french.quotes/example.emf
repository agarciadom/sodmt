@namespace(uri="eclipse.org/epsilon/forums/french.quotes", prefix="fq")
package frenchQuotes;

@gmf.diagram(foo="bar")
class Diagram {
   val Node[*] nodes;
   val Edge[*] edges;
}

@gmf.node(label="name", label.pattern="«Node» {0}")
class Node {
   attr String name;
}

@gmf.link(source="source", target="target")
class Edge {
   ref Node source;
   ref Node target;
}
