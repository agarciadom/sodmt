@data size
operation modelSizes() {
  return 1.to(5);
}

@setup
operation createModel() {
  var m := new Model;
  for (x in 1.to(size)) {
    var e  := new Element;
    e.name := 'e' + x;
    m.elements.add(e);
  }
}

@test
operation correctModelSize() {
  var m := Model.allInstances.first();
  assertEquals(size, m.elements.size());
}
